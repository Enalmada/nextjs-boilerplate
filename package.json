    "@nextui-org/react": "^2.1.7",
    "@pothos/core": "^3.33.0",
    "@pothos/plugin-errors": "^3.11.1",
    "@pothos/plugin-with-input": "^3.10.1",
    "@react-aria/ssr": "^3.7.1",
    "@react-aria/visually-hidden": "^3.8.3",
    "@sentry/nextjs": "7.64.0",
