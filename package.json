{
  "name": "t3-challenge",
  "version": "0.1.0",
  "scripts": {
    "analyze": "cross-env ANALYZE=true pnpm run buildLocal",
    "build": "next build",
    "buildLocal": "dotenv -e ./.env.local -e ./.env.development cross-env NODE_ENV=production next build",
    "check-types": "tsc",
    "check-types-watch": "pnpm run check-types -- --watch --preserveWatchOutput",
    "codegen": "pnpm graphql-codegen --watch",
    "dev": "start-server-and-test start:dev http://localhost:3000 codegen",
    "dev:init": "start-server-and-test start:full http://localhost:3000 codegen",
    "docker:down": "docker compose --env-file .env.development down",
    "docker:up": "docker compose --env-file .env.development up -d",
    "format": "prettier --log-level warn --write .",
    "format:check": "prettier --check .",
    "improve": "node openai.js --overwrite",
    "lint": "dotenv -e ./.env.local -e ./.env.development next lint  && pnpm format:check && && pnpm check-types",
    "lint:fix": "pnpm format && dotenv -e ./.env.local -e ./.env.development next lint --fix && pnpm check-types",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "postinstall": "prisma generate",
    "prepare": "husky install",
    "prisma:migrate": "dotenv -e ./.env.local -e ./.env.development prisma migrate dev",
    "prisma:reset": "dotenv -e ./.env.local -e ./.env.development prisma migrate reset dev",
    "review": "node openai.js",
    "reviewStaged": "sh reviewStaged.sh",
    "start": "next start",
    "start:dev": "next dev",
    "start:full": "pnpm docker:up && pnpm install && pnpm prisma:migrate && next dev",
    "updates": "pnpm outdated -r",
    "vercel-build": "prisma generate && prisma migrate deploy && next build"
  },
