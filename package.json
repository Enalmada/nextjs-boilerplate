{
  "name": "t3-challenge",
  "version": "0.1.0",
  "scripts": {
    "analyze": "cross-env ANALYZE=true pnpm run buildLocal",
    "aws:sso:login": "aws sso login --profile=lbc-dev",
    "build": "next build",
    "build-storybook": "storybook build",
    "buildLocal": "dotenv -e ./.env.local cross-env NODE_ENV=production next build",
    "cf:build": "pnpx @cloudflare/next-on-pages",
    "cf:build:watch": "pnpx @cloudflare/next-on-pages --watch",
    "cf:clean": "ps aux | grep \"@cloudflare/workerd-linux-64/bin/workerd serve\" | grep -v grep | awk '{print $2}' | xargs kill -9",
    "cf:dev": "pnpx wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "check-types": "tsc",
    "check-types-watch": "pnpm run check-types -- --watch --preserveWatchOutput",
    "chromatic": "dotenv -e ./.env.local pnpx chromatic --only-changed",
    "codegen": "pnpm graphql-codegen --watch",
    "create-env": "printenv > .env.local",
    "dev": "pnpm dev:init && start-server-and-test dev:only http-get://localhost:3000/api/graphql codegen",
    "dev:init": "pnpm install && pnpm docker:up && pnpm drizzle:migrate",
    "dev:only": "next dev",
    "docker:down": "docker compose --env-file .env.local down",
    "docker:up": "docker compose --env-file .env.local up -d --remove-orphans",
    "drizzle:drop": "dotenv -e ./.env.local drizzle-kit drop",
    "drizzle:generate": "dotenv -e ./.env.local drizzle-kit generate:pg",
    "drizzle:migrate": "dotenv -e ./.env.local ts-node src/server/db/migrate/migrate",
    "drizzle:push": "drizzle-kit push:postgres",
    "format": "prettier --log-level warn --write .",
    "format:check": "prettier --check .",
    "improve": "node openai.js --overwrite",
    "info": "dotenv -e ./.env.local pnpx next info",
    "lint": "dotenv -e ./.env.local next lint  && pnpm format:check && && pnpm check-types",
    "lint:fix": "pnpm format && dotenv -e ./.env.local next lint --fix && pnpm check-types",
    "pnpm:audit": "pnpm audit --fix",
    "pnpm:depcheck": "depcheck",
    "pnpm:outdated": "pnpm outdated -r",
    "pnpm:prune": "pnpm store prune",
    "prepare": "husky install",
    "prisma:migrate": "dotenv -e ./.env.local prisma migrate dev",
    "prisma:reset": "dotenv -e ./.env.local prisma migrate reset dev",
    "review": "node openai.js",
    "reviewStaged": "sh reviewStaged.sh",
    "sst:dev": "sst bind next dev",
    "sst:dev:deploy": "rm -rf .next && sst deploy --stage dev",
    "sst:dev:init": "cross-env AWS_PROFILE=lbc-dev npx sst dev",
    "start": "next start",
    "storybook": "storybook dev -p 3001 --no-open --quiet",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test.local -e .env.test -- vitest",
    "test:coverage": "cross-env NODE_ENV=test dotenv -e .env.test.local -e .env.test -- vitest --coverage",
    "test:watch": "cross-env NODE_ENV=test dotenv -e .env.test.local -e .env.test -- vitest watch",
    "vercel-build": "pnpm run drizzle:migrate && next build",
    "vercel:buildLocal": "dotenv -e ./.env.local pnpx vercel build"
  },
  "dependencies": {
    "@apollo/client": "^3.8.1",
    "@apollo/experimental-nextjs-app-support": "^0.4.1",
    "@ducanh2912/next-pwa": "^9.5.0",
    "@envelop/generic-auth": "^6.0.0",
